function showMessage(message, type = 'danger') {
    $.notify({                                                                                          // Уведомление о действии
        message: message
    }, {
        element: 'body',
        placement: {
            from: "top",
            align: "right"
        },
        allow_duplicates: false,
        showAnimation: 'slideDown',
        showDuration: 400,
        hideAnimation: 'slideUp',
        hideDuration: 200,
        gap: 2,
        offset: 50,
        spacing: 10,
        z_index: 1051,
        delay: 3000,
        type: type
    });
}                                               

